{
  "name": "census2020",
  "version": "1.0.0",
  "description": "React application for Census2020 Kiosk",
  "main": "server.js",
  "scripts": {
    "start": "per-env",
    "prebuild": "rimraf ./build && npm run build-server-react && npm run translation-site:install-if-enabled",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.prod.config.js",
    "start:production": "cross-env NODE_ENV=production node --icu-data-dir=node_modules/full-icu ./server/server.js",
    "start:development": "cross-env NODE_ENV=development nodemon --icu-data-dir=node_modules/full-icu ./server/server.js",
    "start:translation": "cross-env NODE_ENV=production npm run translation-site:install && cross-env NODE_ENV=production npm run translation-site:build && cross-env NODE_ENV=production npm run translation-site",
    "i18n:extract": "node ./i18n/extract-strings.js",
    "i18n:check": "node ./i18n/check-translations.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prebuild-server-react": "rimraf ./server/mail/mail-components/compiled",
    "build-server-react": "cross-env NODE_ENV=production webpack --config=server/mail/mail-components/webpack.config.js",
    "translation-site": "per-env",
    "translation-site:install-if-enabled": "if-env-defined CENSUS2020_SERVE_TRANSLATE_SITE && npm run translation-site:install && npm run translation-site:build || exit 0",
    "translation-site:install": "npm install --prefix translate-site",
    "translation-site:build": "npm run build --prefix translate-site",
    "translation-site:development": "cross-env NODE_ENV=development npm start --prefix translate-site",
    "translation-site:production": "cross-env NODE_ENV=production npm start --prefix translate-site"
  },
  "author": "Code for San Jose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codeforsanjose/census2020/issues"
  },
  "homepage": "https://github.com/codeforsanjose/census20208",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@formatjs/intl-pluralrules": "^1.3.2",
    "@formatjs/intl-relativetimeformat": "^4.4.1",
    "@google-cloud/translate": "^5.1.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-intl": "^3.3.3",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.1",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "express": "^4.16.4",
    "express-responsive-images": "^1.1.0",
    "file-loader": "^4.1.0",
    "file-saver": "^2.0.2",
    "full-icu": "^1.3.0",
    "hast-util-find-and-replace": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "if-env-defined": "^1.0.0",
    "intl": "^1.2.5",
    "jszip": "^3.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.1",
    "nodemailer": "^6.2.1",
    "per-env": "^1.0.2",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^3.4.0",
    "react-remarkable": "^1.1.3",
    "react-responsive-carousel": "^3.1.51",
    "react-router-dom": "^5.0.0",
    "react-toastify": "^5.3.2",
    "react-youtube": "^7.9.0",
    "remark-parse": "^7.0.2",
    "remark-rehype": "^5.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "unified": "^8.4.2",
    "uswds": "^2.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.2"
  },
  "devDependencies": {
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.19.4",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.10.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && git add ."
    }
  },
  "lint-staged": {
    "*.js": "npm run lint:fix"
  }
}
